<div class="row">
  <a href="/user/new" class="btn-floating waves-effect waves-light"><i class="material-icons">add</i></a>
  <div class="col s12">
    <table>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>User Name</th>
      <th>Permission</th>
    </tr>
    {{#each data}}
      <tr>
        <td>{{firstName}}</td>
        <td>{{lastName}}</td>
        <td>{{userName}}</td>
        <td>{{permission}}</td>
      </tr>
    {{/each}}
  </table>
  </div>
</div>